{
  "extends": "eslint-config-airbnb",
  "env": {
    "node": true,
    "jest/globals": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "plugins": ["jest"],
  "rules": {
    "comma-dangle": 0,
    "no-prototype-builtins": 0
  }
}
